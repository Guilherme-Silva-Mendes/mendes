{
  "Function": {
    "prefix": "fn",
    "body": [
      "fn ${1:name}(${2:params}) -> ${3:type}:",
      "    ${0}"
    ],
    "description": "Create a function"
  },
  "Async Function": {
    "prefix": "afn",
    "body": [
      "async fn ${1:name}(${2:params}) -> ${3:type}:",
      "    ${0}"
    ],
    "description": "Create an async function"
  },
  "Struct": {
    "prefix": "struct",
    "body": [
      "struct ${1:Name}:",
      "    ${2:field}: ${3:type}",
      "    ${0}"
    ],
    "description": "Create a struct"
  },
  "Impl": {
    "prefix": "impl",
    "body": [
      "impl ${1:Type}:",
      "    fn ${2:method}(&self) -> ${3:type}:",
      "        ${0}"
    ],
    "description": "Create an impl block"
  },
  "Enum": {
    "prefix": "enum",
    "body": [
      "enum ${1:Name}:",
      "    ${2:Variant1}",
      "    ${3:Variant2}",
      "    ${0}"
    ],
    "description": "Create an enum"
  },
  "API GET": {
    "prefix": "apiget",
    "body": [
      "api GET ${1:/path}:",
      "    ${0}",
      "    return json(${2:data})"
    ],
    "description": "Create a GET endpoint"
  },
  "API POST": {
    "prefix": "apipost",
    "body": [
      "api POST ${1:/path}:",
      "    let ${2:body} = req.json::<${3:Type}>()",
      "    ${0}",
      "    return json(${4:result}).status(201)"
    ],
    "description": "Create a POST endpoint"
  },
  "API PUT": {
    "prefix": "apiput",
    "body": [
      "api PUT ${1:/path/{id}}:",
      "    let ${2:body} = req.json::<${3:Type}>()",
      "    ${0}",
      "    return json(${4:result})"
    ],
    "description": "Create a PUT endpoint"
  },
  "API DELETE": {
    "prefix": "apidel",
    "body": [
      "api DELETE ${1:/path/{id}}:",
      "    ${0}",
      "    return status(204)"
    ],
    "description": "Create a DELETE endpoint"
  },
  "Server Config": {
    "prefix": "server",
    "body": [
      "server:",
      "    host \"${1:127.0.0.1}\"",
      "    port ${2:8080}"
    ],
    "description": "Create server configuration"
  },
  "Database Config": {
    "prefix": "db",
    "body": [
      "db:",
      "    driver \"${1|postgres,mysql,sqlite|}\"",
      "    host \"${2:localhost}\"",
      "    database \"${3:mydb}\"",
      "    pool_size ${4:10}"
    ],
    "description": "Create database configuration"
  },
  "If Statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition}:",
      "    ${0}"
    ],
    "description": "Create an if statement"
  },
  "If Else": {
    "prefix": "ife",
    "body": [
      "if ${1:condition}:",
      "    ${2}",
      "else:",
      "    ${0}"
    ],
    "description": "Create an if-else statement"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:item} in ${2:collection}:",
      "    ${0}"
    ],
    "description": "Create a for loop"
  },
  "While Loop": {
    "prefix": "while",
    "body": [
      "while ${1:condition}:",
      "    ${0}"
    ],
    "description": "Create a while loop"
  },
  "Match": {
    "prefix": "match",
    "body": [
      "match ${1:expr}:",
      "    ${2:Pattern1}:",
      "        ${3}",
      "    ${4:Pattern2}:",
      "        ${0}"
    ],
    "description": "Create a match expression"
  },
  "Match Option": {
    "prefix": "matchopt",
    "body": [
      "match ${1:option}:",
      "    Some(${2:value}):",
      "        ${3}",
      "    None:",
      "        ${0}"
    ],
    "description": "Match an Option type"
  },
  "Match Result": {
    "prefix": "matchres",
    "body": [
      "match ${1:result}:",
      "    Ok(${2:value}):",
      "        ${3}",
      "    Err(${4:e}):",
      "        ${0}"
    ],
    "description": "Match a Result type"
  },
  "Let": {
    "prefix": "let",
    "body": [
      "let ${1:name}: ${2:type} = ${0}"
    ],
    "description": "Create a variable"
  },
  "Let Mutable": {
    "prefix": "letm",
    "body": [
      "let mut ${1:name}: ${2:type} = ${0}"
    ],
    "description": "Create a mutable variable"
  },
  "Middleware": {
    "prefix": "middleware",
    "body": [
      "middleware ${1:name}:",
      "    ${0}",
      "    next()"
    ],
    "description": "Create a middleware"
  },
  "Trait": {
    "prefix": "trait",
    "body": [
      "trait ${1:Name}:",
      "    fn ${2:method}(&self) -> ${3:type}",
      "    ${0}"
    ],
    "description": "Create a trait"
  },
  "Print": {
    "prefix": "print",
    "body": [
      "print(\"${1:message}\")"
    ],
    "description": "Print to console"
  },
  "DB Query": {
    "prefix": "dbquery",
    "body": [
      "let ${1:result} = await db.query(\"${2:SELECT * FROM table}\")"
    ],
    "description": "Database query"
  },
  "DB Query One": {
    "prefix": "dbqueryone",
    "body": [
      "let ${1:result} = await db.query_one(\"${2:SELECT * FROM table WHERE id = $1}\", [${3:id}])"
    ],
    "description": "Database query single row"
  },
  "JSON Response": {
    "prefix": "json",
    "body": [
      "return json(${1:data})"
    ],
    "description": "Return JSON response"
  },
  "Error Response": {
    "prefix": "error",
    "body": [
      "return status(${1:400}).json({ \"error\": \"${2:message}\" })"
    ],
    "description": "Return error response"
  }
}
